	processor 6502
; start at 80:00
	org $8000

	LDX #$0
optelx
	INX			; x gaat optellen
	CPX #$6		; x gaat tot 6 en stopt dan met optellen
	BNE optelx
aftelx
	DEX
	CPX #$2		; x telt af tot 1
	BMI aftelx

	;zelfde voor y
	
	LDY #$0
optely
	INY			; y gaat optellen
	CPY #$6		; y gaat tot 6 en stopt dan met optellen
	BNE optely
aftely
	DEY
	CPY #$2		; y telt af tot 1
	BMI aftely
	JMP vervolg ; spring naar vervolg code
error
	LDE #$20
	DLY #$20
	LDA #$20
	dc $de
vervolg
	LDA #$-20
	BPL error:		; zou niet moeten springen
	CLC
	ADC #$Oxffff	;moet overflow setten
	BVS carry_test	;zou jump moeten maken
carry_test
	SEC 
	BCC error
	CLC
	BCS error
	BCC carry2
	JMP error
carry2
	SEC
	BCS bittest
	JMP error
bittest
	LDA #$0xf0 
	STA $0x0200
	LDA #$0x0f
	BIT $0x0200	
	BEQ bittest2
    JMP error
bittest
	LDA #$0x10
	BIT $0x0200
	BEQ error
	JSR sub

	dc $de
sub
	LDA#$50
	RTS